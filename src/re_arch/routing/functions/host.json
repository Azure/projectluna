{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:01",
      "visibilityTimeout": "00:00:01",
      "batchSize": 1,
      "maxDequeueCount": 5,
      "messageEncoding": "base64"
    }
  },
  "logging": {
    "logLevel": {
      "default": "Debug"
    },
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": false
      }
    }
  }
}